<div>
  <h2>Seeker</h2>
  <table class="table">
    <thead>
      <tr>
        <th></th>
        <th>Name</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Age</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @resumes.each do |resume| %>
        <tr>
          <td><%= link_to (image_tag resume.cover.url, size: "30x30"), asset_path(resume.cover.url), target: "blank" %></td>
          <td><%= resume.fullname %></td>
          <td><%= resume.user.email %></td>
          <td><%= resume.phone %></td>
          <td><%= resume.age %></td>
          <td><%= link_to "Edit", "/admin/edit_seeker/#{resume.id}", class: "btn btn-success" %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
